<?xml version="1.0" encoding="UTF-8"?>
<splashy xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://splashy.alioth.debian.org/schemas/1.1/theme.xsd">
    <info>
	<name>lightgreyv2-beastie</name>
	<version>1.0</version>
        <description>matching theme for my LightGreyV2 XFCE-Style</description>
        <urls>http://sixsixfive.deviantart.com</urls>
	<author>PUNX69</author>
    </info>
    <progressbar>
        <dimension>
		<x>25</x>
		<y>82</y>
		<width>50</width>
		<height>3</height>
        </dimension>
        <color>
		<red>68</red>
		<green>68</green>
		<blue>68</blue>
		<alpha>255</alpha>
        </color>
        <border>
   		<enable>no</enable>
		<color>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
			<alpha>0</alpha>
		</color>
        </border>
        <background>
            <color>
                <red>239</red>
                <green>239</green>
                <blue>239</blue>
                <alpha>0</alpha>
            </color>
        </background>
	<direction>
		<boot>forward</boot>
		<shutdown>backward</shutdown>
		<resume>forward</resume>
		<suspend>backward</suspend>
	</direction>
	<visibility>
		<boot>yes</boot>
		<shutdown>yes</shutdown>
		<resume>yes</resume>
		<suspend>yes</suspend>
	</visibility>
    </progressbar>
    <background>
	<boot>background.png</boot>
	<shutdown>background.png</shutdown>
	<resume>background.png</resume>
	<suspend>background.png</suspend>
	<errorimg>error.png</errorimg>
	<dimension>
		<x>0</x>
		<y>0</y>
		<width>0</width>
		<height>0</height>
	</dimension>
    </background>
    <textbox>
	<enable>no</enable>
        <dimension>
		<x>0</x>
		<y>0</y>
		<width>0</width>
		<height>0</height>
        </dimension>
        <color>
		<red>0</red>
		<green>0</green>
		<blue>0</blue>
		<alpha>0</alpha>
        </color>
        <border>
		<enable>no</enable>
		<color>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
			<alpha>0</alpha>
		</color>
	</border>
	<text>
		<font>
			<file>NADA</file>
			<height>0</height>
		</font>
		<color>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
			<alpha>0</alpha>
		</color>
	</text>
    </textbox>
    <autoverboseonerror>no</autoverboseonerror>
    <fadein>no</fadein>
    <fadeout>no</fadeout>
</splashy>