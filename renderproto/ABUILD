pkgname=renderproto
pkgver=0.11.1
pkgbuild=1
arch=('auto')

shortdesc=('renderproto')
longdesc=('renderproto is part of X11. For more information about the X.Org Foundation (the providers of the X.Org implementation of the X Window System), see their website: http://www.x.org')

tags=('x11-protos xserver')

source=("http://xorg.freedesktop.org/archive/individual/proto/renderproto-${pkgver}.tar.bz2")

build_deps='x11-skel'

before_build() {
 echo ""
}

BUILD_SYSTEM='autotools'
BUILD_KEYS="--prefix=/usr --libdir=/usr/lib${LIBDIRSUFFIX} --sysconfdir=/etc --localstatedir=/var --infodir=/usr/info --mandir=/usr/man --disable-static --with-fontrootdir=/usr/share/fonts --build=$ARCH-slackware-linux "
INSTALL_KEYS="DESTDIR=$pkgdir"
after_build() {
 echo ""
}

