pkgname=squashfs-fluxbox
pkgver=1.1.1
pkgbuild=2
arch=('noarch')

shortdesc="Script for acceleration and compression"
#longdesc=(""
#)

source=("My hand")


tags="base app-admin"

build_deps="mkpkg"
adddep="squashfs-tools"


build(){
  mkdir -p  $pkgdir/sbin
  cp $startdir/hand/* $pkgdir/sbin
  chmod 755 $pkgdir/sbin/*
}


